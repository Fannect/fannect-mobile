{
	"author": "blakevanlan@gmail.com", 
	"config_version": "2", 
	"description": "Social Network for Sports Fans", 
	"homepage": "http://fannect.me", 
	"modules": {
		"display": {
			"orientations": {
				"android": "portrait", 
				"ipad": "portrait", 
				"iphone": "portrait"
			}
		}, 
		"event": true, 
		"file": true, 
		"geolocation": true, 
		"icons": {
			"android": {
				"36": "res/icon-android-36.png", 
				"48": "res/icon-android-48.png", 
				"72": "res/icon-android-72.png"
			},
			"ios": {
				"57": "res/icon-ios-57.png",
            "72": "res/icon-ios-72.png",
            "96": "res/icon-ios-96.png",
            "114": "res/icon-ios-114.png",
            "144": "res/icon-ios-144.png",
            "512": "res/icon-ios-512.png",
            "prerendered": true
         }
		}, 
		"is": true, 
		"launchimage": {
			"android": "res/screen-1024x768.png", 
			"android-landscape": "res/screen-1024x768.png", 
			"background-color": "#000000"
		}, 
		"logging": {
			"level": "WARNING"
		}, 
		"media": true, 
		"notification": true, 
		"prefs": true, 
		"reload": true, 
		"request": {
			"permissions": [
				"http://*/*", 
				"https://*/*"
			]
		}, 
		"tabbar": true, 
		"tools": true, 
		"topbar": true,
		"package_names": {
			"ios": "com.fannect.*"
		}
	}, 
	"name": "Fannect Mobile", 
	"platform_version": "v1.4", 
	"version": "0.1"
}